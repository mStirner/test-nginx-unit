{
    "listeners": {
        "*:8080": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "method": "GET",
                "uri": "^/(?<app_name>[^/]+)"
            },
            "action": {
                "share": "applications/$app_name"
            }
        }
    ]
}
